load(
    "@io_bazel_rules_rust//rust:rust.bzl",
    "rust_binary",
)
load("@rust_deps//:defs.bzl", "crate")

rust_binary(
    name = "has_aliased_deps",
    srcs = ["src/bin.rs"],
    crate_type = "bin",
    edition = "2018",
    deps = [
        crate("reqwest"),
    ],
)

sh_test(
    name = "test",
    srcs = ["test.sh"],
    data = [":has_aliased_deps"],
)
